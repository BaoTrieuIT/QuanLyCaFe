	<queryString>
		<![CDATA[SELECT
     DoUong."MaMon" AS DoUong_MaMon,
     DoUong."MaNhaCC" AS DoUong_MaNhaCC,
     DoUong."TenMon" AS DoUong_TenMon,
     DoUong."Sizemon" AS DoUong_Sizemon,
     DoUong."GiaBan" AS DoUong_GiaBan,
     DoUong."SoLuong" AS DoUong_SoLuong,
     HOADON."MaHD" AS HOADON_MaHD,
     HOADON."MaNV" AS HOADON_MaNV,
     HOADON."TongTienHD" AS HOADON_TongTienHD,
     HOADON."GhiChu" AS HOADON_GhiChu,
     HOADONCHITIET."MaHDCT" AS HOADONCHITIET_MaHDCT,
     HOADONCHITIET."MaHD" AS HOADONCHITIET_MaHD,
     HOADONCHITIET."MaMon" AS HOADONCHITIET_MaMon,
     HOADONCHITIET."Soluong" AS HOADONCHITIET_Soluong,
     HOADONCHITIET."Thanhtien" AS HOADONCHITIET_Thanhtien,
     HOADONCHITIET."GHiChu" AS HOADONCHITIET_GHiChu,
     NHANVIEN."MaNV" AS NHANVIEN_MaNV,
     NHANVIEN."HoTen" AS NHANVIEN_HoTen,
     HOADON."NgaylapHD" AS HOADON_NgaylapHD
FROM
     "dbo"."DoUong" DoUong INNER JOIN "dbo"."HOADONCHITIET" HOADONCHITIET ON DoUong."MaMon" = HOADONCHITIET."MaMon"
     INNER JOIN "dbo"."HOADON" HOADON ON HOADONCHITIET."MaHD" = HOADON."MaHD"
     INNER JOIN "dbo"."NHANVIEN" NHANVIEN ON HOADON."MaNV" = NHANVIEN."MaNV"
WHERE HOADON."MaHD" =  $P{MaHD}]]>
	</queryString>
